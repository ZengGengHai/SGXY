<map class="goToMap"
  markers="{{ markers }}"
  controls="{{ controls }}"
  longitude="{{ mapLongitude }}"
  latitude="{{ mapLatitude }}"
  bindcontroltap="controlTap"
  polyline="{{ polyline }}"
  scale="18"
  include-points="{{ includePoints }}"
  show-location 
>
<canui-head-box></canui-head-box>
</map>